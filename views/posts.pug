extends layout 

block content 
  h1 Clubhouse Posts 
  if user
    a(href="/post/new")
      button Add New Post  
  for post in posts 
    div 
      p= post.title 
      if user && user.isMember
        p= post.createdBy.username 
      if user && user.isMember
        p= post.date 
      p= post.message